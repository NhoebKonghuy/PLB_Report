        <Field Name="application_no">
          <DataField>application_no</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="village_code">
          <DataField>village_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
