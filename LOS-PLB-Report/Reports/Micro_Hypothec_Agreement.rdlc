    <DataSet Name="CO_BORROWER">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
	* 
FROM
	(
	SELECT ROW_NUMBER
		( ) OVER ( ORDER BY ag.ID ) AS num,
  AP.APPLICATION_NO,
  'និងឈ្មោះ  '||CC.FAMILY_NAME_KH|| ' ' || CC.GIVEN_NAME_KH ||' ភេទ '||
